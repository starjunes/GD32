/*
-------------------------------IO类注册信息--------------------------------------
    BEGIN_OUTPUT_DEF(_TYPE_)
    _TYPE_:   该IO类父设备，见PE_TYPE_E，OUTPUT_TYPE_MAIN为主板
********************************************************************************-
*/

/*
-------------------------------IO注册信息--------------------------------------
    OUTPUT_DEF(_TYPE_, _PORT_ID_, _PIN_, _LEVEL_, _INIT_, _PULLUP_, _PULLDOWN_)
    _TYPE_:      该IO类父设备，见PE_TYPE_E，OUTPUT_TYPE_MAIN为主板
    _PORT_ID_:   统一IO口编号
    _PIN_:       对应GPIO管脚
    _LEVEL_：    初始电平
    _INIT_：     该IO口初始化函数
    _PULLUP_：   输出高电平函数
    _PULLDOWN_:  输出低电平函数
********************************************************************************-
*/

//-----------------主板IO输入---------------------------------------------------
BEGIN_OUTPUT_DEF(OUTPUT_TYPE_MAIN)
    OUTPUT_DEF(OUTPUT_TYPE_MAIN, OPT_LEDRED,   GPIO_PIN_A13, 0, DAL_GPIO_InitOutput,  DAL_GPIO_Pulldown,          DAL_GPIO_Pullup)
    OUTPUT_DEF(OUTPUT_TYPE_MAIN, OPT_LEDGREEN, GPIO_PIN_MAX, 0, DAL_GPIO_InitOutput,  DAL_GPIO_Pullup,            DAL_GPIO_Pulldown)
    //OUTPUT_DEF(OUTPUT_TYPE_MAIN, OPT_LEDRED,   GPIO_PIN_C8, 0,  DAL_GPIO_InitOutput,  DAL_GPIO_Pullup,          DAL_GPIO_Pulldown)
    //OUTPUT_DEF(OUTPUT_TYPE_MAIN, OPT_LEDGREEN, GPIO_PIN_C9, 0,  DAL_GPIO_InitOutput,  DAL_GPIO_Pullup,          DAL_GPIO_Pulldown)
    OUTPUT_DEF(OUTPUT_TYPE_MAIN, OPT_LEDMIX,   GPIO_PIN_MAX, 0, DAL_GPIO_InitOutput,  DAL_GPIO_PullupMixLed,    DAL_GPIO_PulldownMixLed)
END_OUTPUT_DEF(OUTPUT_TYPE_MAIN)


