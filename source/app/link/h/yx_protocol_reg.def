/*
-------------------------------类注册信息--------------------------------------
    BEGIN_PROTOCOL_DEF(_TYPE_)
    _TYPE_:   协议类型
---------------------------------------------------------------------------------
*/

/*
-------------------------------注册信息--------------------------------------
    PROTOCOL_DEF(_TYPE_, _MSG_ID_, _TCP_COM_, _UDP_COM_, _SM_)
    _TYPE_:   协议类型
    _MSG_ID_: 协议ID
    _TCP_COM_:TCP通道
    _UDP_COM_:UDP通道
    _SM_:     是否需要短信通道备份
---------------------------------------------------------------------------------
*/

//-----------------电话状态通知，按照注册先后顺序执行----------------------------
BEGIN_PROTOCOL_DEF(PROTOCOL_TYPE_CMD)
    /* 信息点播开关 */
    PROTOCOL_DEF(PROTOCOL_TYPE_CMD, UP_CMD_INFOSWITCH,      (1 << SOCKET_CH_0) | (1 << CHA_JT2_TLINK), (1 << CHA_JT1_ULINK) | (1 << CHA_JT2_ULINK), 0)
    /* 周边信息查询 */
    PROTOCOL_DEF(PROTOCOL_TYPE_CMD, UP_CMD_NEAR_INFOQRYREQ, (1 << SOCKET_CH_0) | (1 << CHA_JT2_TLINK), (1 << CHA_JT1_ULINK) | (1 << CHA_JT2_ULINK), 0)
    /* 上行透传数据,北斗招标 */
    #if EN_BD_INVITE > 0
    PROTOCOL_DEF(PROTOCOL_TYPE_CMD, UP_CMD_TRANSINFO,       (1 << SOCKET_CH_0) | (1 << CHA_JT2_TLINK), (1 << CHA_JT1_ULINK) | (1 << CHA_JT2_ULINK), 0)
    #endif
    
    /* 事件上报 */
    PROTOCOL_DEF(PROTOCOL_TYPE_CMD, UP_CMD_EVENTREPORT,     (1 << SOCKET_CH_0) | (1 << CHA_JT2_TLINK), (1 << CHA_JT1_ULINK) | (1 << CHA_JT2_ULINK), 0)
    /* 电子运单上报 */
    PROTOCOL_DEF(PROTOCOL_TYPE_CMD, UP_CMD_EBILL,           (1 << SOCKET_CH_0) | (1 << CHA_JT2_TLINK), (1 << CHA_JT1_ULINK) | (1 << CHA_JT2_ULINK), 0)
    /* 提问应答 */
    PROTOCOL_DEF(PROTOCOL_TYPE_CMD, UP_ACK_ASK,             (1 << SOCKET_CH_0) | (1 << CHA_JT2_TLINK), (1 << CHA_JT1_ULINK) | (1 << CHA_JT2_ULINK), 0)
END_PROTOCOL_DEF(PROTOCOL_TYPE_CMD)



