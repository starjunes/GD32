/*
-------------------------------通道注册信息--------------------------------------
    ADC_DEF(_CH_, _PIN_ADC_, _ADC_CH_, _DMA_BASE,  _ADC_BASE, _GPIO_BASE, _RCC)
    _CH_:          统一通道编号
    _PIN_ADC_：    发送管脚
    _ADC_CH_:      ADC物理通道
    _DMA_BASE：    接收管脚
    _ADC_BASE：    ADC寄存器基准地址,模拟ADC则填0
    _GPIO_BASE：   GPIO寄存器基准地址
    _RCC：         ADC系统时钟,模拟ADC则填0
********************************************************************************-
*/


BEGIN_ADC_CFG
    ADC_DEF(ADC_CH_0, GPIO_PinSource7,  ADC_Channel_7,  DMA1_Channel1_BASE,  ADC1_BASE, GPIOA_BASE, RCC_APB2Periph_ADC1) /* 主电源电压检测 */
#if EN_UARTEXT == 0
    ADC_DEF(ADC_CH_1, GPIO_PinSource4,  ADC_Channel_14, DMA1_Channel1_BASE,  ADC1_BASE, GPIOC_BASE, RCC_APB2Periph_ADC1) /* ADC1 */
    ADC_DEF(ADC_CH_2, GPIO_PinSource5,  ADC_Channel_15, DMA1_Channel1_BASE,  ADC1_BASE, GPIOC_BASE, RCC_APB2Periph_ADC1) /* ADC2 */
#endif
END_ADC_CFG

