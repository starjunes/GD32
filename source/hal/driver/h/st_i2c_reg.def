/*
-------------------------------通道注册信息--------------------------------------
    I2C_DEF(_COM_, _ENABLE, _PIN_SCL, _PIN_SDA,  _I2C_BASE, _GPIO_BASE, _RCC)
    _COM_:         统一通道编号
    _ENABLE：      使能
    _PIN_SCL：     发送管脚
    _PIN_SDA：     接收管脚
    _I2C_BASE：    I2C寄存器基准地址,模拟I2C则填0
    _GPIO_BASE：   GPIO寄存器基准地址
    _RCC：         I2C系统时钟,模拟I2C则填0
********************************************************************************-
*/


BEGIN_I2C_CFG
    I2C_DEF(I2C_COM_0, 1, GPIO_PinSource10, GPIO_PinSource11,  I2C2_BASE, GPIOB_BASE, RCC_APB1Periph_I2C2)
    I2C_DEF(I2C_COM_1, 0, GPIO_PinSource8,  GPIO_PinSource6,   0,         GPIOC_BASE, 0)
END_I2C_CFG

