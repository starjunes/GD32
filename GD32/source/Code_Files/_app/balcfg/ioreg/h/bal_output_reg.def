/*
-------------------------------IO类注册信息--------------------------------------
    BEGIN_OUTPUT_DEF(_TYPE_)
    _TYPE_:   该IO类父设备，见PE_TYPE_E，OUTPUT_TYPE_MAIN为主板
---------------------------------------------------------------------------------
*/

/*
-------------------------------IO注册信息--------------------------------------
    OUTPUT_DEF(_TYPE_, _PORT_ID_, _LEVEL_, _INIT_, _PULLUP_, _PULLDOWN_)
    _TYPE_:      该IO类父设备，见PE_TYPE_E，OUTPUT_TYPE_MAIN为主板
    _PORT_ID_:   统一IO口编号
    _LEVEL_：    初始电平
    _INIT_：     该IO口初始化函数
    _PULLUP_：   输出高电平函数
    _PULLDOWN_:  输出低电平函数
---------------------------------------------------------------------------------
*/

//-----------------主板IO输出---------------------------------------------------
BEGIN_OUTPUT_DEF(OUTPUT_TYPE_MAIN)
    OUTPUT_DEF(OUTPUT_TYPE_MAIN, PORT_GPSLED,   0, bal_Init_PIN_GPSLED,  bal_Pullup_GPSLED, bal_Pulldown_GPSLED)
    OUTPUT_DEF(OUTPUT_TYPE_MAIN, PORT_CANLED,   0, bal_Init_PIN_CANLED,  bal_Pullup_CANLED, bal_Pulldown_CANLED)
    OUTPUT_DEF(OUTPUT_TYPE_MAIN, PORT_WIFILED,	0, bal_Init_PIN_WIFILED, bal_Pullup_WIFILED,bal_Pulldown_WIFILED)

	END_OUTPUT_DEF(OUTPUT_TYPE_MAIN)

