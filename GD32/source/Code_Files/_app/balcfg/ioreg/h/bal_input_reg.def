/*
-------------------------------IO类注册信息--------------------------------------
    BEGIN_INPUT_DEF(_FATYPE_)
    _FATYPE_:   该IO类父设备，见PE_TYPE_E，PE_TYPE_NOUSED为主板
---------------------------------------------------------------------------------
*/
/*
-------------------------------IO注册信息--------------------------------------
    #define INPUT_DEF(_PORT_ID_, _LOW_TIME_, _HIGH_TIME_, _INIT_, _READ_)
    _PORT_ID_:   统一IO口编号
    _LOW_TIME_:  该IO口低电平滤波时间，单位：毫秒，必须为100ms的整数倍
    _HIGH_TIME_：该IO口高电平滤波时间，单位：毫秒，必须为100ms的整数倍
    _INIT_：     该IO口初始化函数
    _READ_：     该IO口状态读取函数
---------------------------------------------------------------------------------
*/

//-----------------主板IO输入---------------------------------------------------
BEGIN_INPUT_DEF(PE_TYPE_NOUSED, bal_input_SetFilterPara)
    INPUT_DEF(PE_TYPE_NOUSED, TYPE_ACC,      30,     30,   bal_InitPort_ACC,      bal_ReadPort_ACC)
    INPUT_DEF(PE_TYPE_NOUSED, TYPE_ECALL,    10,     10,   bal_InitPort_ECALL,    bal_ReadPort_ECALL)
    INPUT_DEF(PE_TYPE_NOUSED, TYPE_BCALL,    10,     10,   bal_InitPort_BCALL,    bal_ReadPort_BCALL)
    INPUT_DEF(PE_TYPE_NOUSED, TYPE_PWRDECT,  5000,   5000, bal_InitPort_PWRDECT,  bal_ReadPort_PWRDECT)
    INPUT_DEF(PE_TYPE_NOUSED, TYPE_GPSOPEN,  10,     10,   bal_InitPort_GPSOPEN,  bal_ReadPort_GPSOPEN)
    INPUT_DEF(PE_TYPE_NOUSED, TYPE_GPSSHORT, 10,     10,   bal_InitPort_GPSSHORT, bal_ReadPort_GPSSHORT)

    INPUT_DEF(PE_TYPE_NOUSED, TYPE_CHG,      10,     10,   bal_InitPort_CHG,      bal_ReadPort_CHG)
    INPUT_DEF(PE_TYPE_NOUSED, TYPE_VINLOW,   5000,   5000, bal_InitPort_VINLOW,   bal_ReadPort_VINLOW)
END_INPUT_DEF(PE_TYPE_NOUSED)

